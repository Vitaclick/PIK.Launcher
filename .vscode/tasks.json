{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/PIK.Launcher.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "release",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/PIK.Launcher.csproj",
        "-c",
        "Release",
        "-r",
        "win-x64",
        "--self-contained",
        "true",
        "-o",
        "L:\\09_Программы\\Assembler\\Launcher"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
